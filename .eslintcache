;\n    return {\n      x: evt.clientX - rect.left,\n      y: evt.clientY - rect.top\n    }\n}\n\n\n\nconst Overlay = () => {\n    const canvasRef = useRef(null)\n\n    let thingX = 0\n    let thingY = 0\n    let mouseX = 0\n    let mouseY = 0\n    let cycle = 0\n    let direction = new Victor(0, 0).normalize()\n    let radius = 10\n    let rotation = .2\n    let thingSize = 100\n    let prevX = 0 // optimize +_+\n    let prevY = 0\n    let stop = false\n\n    let rotationCycles = [0.1, -0.1]\n    let i = 0\n    \n\n    useEffect(() => {\n        const canvas = canvasRef.current\n        const ctx = canvas.getContext('2d')\n\n        const interval = setInterval(() => {\n            if (!canvasRef.current || stop) return\n            // is this too expensive?\n            // https://stackoverflow.com/questions/4288253/html5-canvas-100-width-height-of-viewport\n            if (canvas.width != window.innerWidth) canvas.width = window.innerWidth\n            if (canvas.height != window.innerHeight) canvas.height = window.innerHeight\n\n            // need to have a switch here based on (i) so can do things other than rotate (this is for rem)\n\n            direction.rotate(rotationCycles[i]).normalize()\n\n            prevX = thingX\n            prevY = thingY\n\n            thingX = mouseX + radius * direction.x + cycle * direction.x\n            thingY = mouseY + radius * direction.y + cycle * direction.y\n\n            cycle += 1\n            if (cycle >= 1000) cycle = 0\n\n            ctx.clearRect(0, 0, canvas.width, canvas.height)\n            // ctx.clearRect(prevX, prevY, thingSize, thingSize)\n            ctx.fillStyle = '#ffcc00'\n            ctx.fillRect(thingX - thingSize / 2, thingY - thingSize / 2, thingSize, thingSize)\n        }, 64)\n\n        return () => clearInterval(interval)\n    }, [])\n\n    const mouseMove = (evt) => {\n        if (!canvasRef.current) return\n        const { x, y } = getMousePos(canvasRef.current, evt)\n        mouseX = x\n        mouseY = y\n    }\n\n    const click = () => {\n        // stop = !stop\n        thingX = mouseX\n        thingY = mouseY\n        cycle = 0\n\n        if (i + 1 >= rotationCycles.length) i = 0\n        else i += 1\n    }\n\n    console.log('?', window, document)\n    \n    return <canvas id=\"canvas\" className=\"overlay\" ref={canvasRef} onMouseMove={mouseMove} onClick={click} />\n}\n\nexport default Overlay",{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":9,"column":8,"nodeType":"57","messageId":"58","endLine":9,"endColumn":15},{"ruleId":"55","severity":1,"message":"59","line":1,"column":36,"nodeType":"57","messageId":"58","endLine":1,"endColumn":44},{"ruleId":"55","severity":1,"message":"60","line":26,"column":9,"nodeType":"57","messageId":"58","endLine":26,"endColumn":17},{"ruleId":"61","severity":1,"message":"62","line":44,"column":30,"nodeType":"63","messageId":"64","endLine":44,"endColumn":32},{"ruleId":"61","severity":1,"message":"62","line":45,"column":31,"nodeType":"63","messageId":"64","endLine":45,"endColumn":33},{"ruleId":"55","severity":1,"message":"65","line":51,"column":13,"nodeType":"57","messageId":"58","endLine":51,"endColumn":18},{"ruleId":"66","severity":1,"message":"67","line":51,"column":21,"nodeType":"57","endLine":51,"endColumn":27},{"ruleId":"55","severity":1,"message":"68","line":52,"column":13,"nodeType":"57","messageId":"58","endLine":52,"endColumn":18},{"ruleId":"66","severity":1,"message":"69","line":52,"column":21,"nodeType":"57","endLine":52,"endColumn":27},{"ruleId":"66","severity":1,"message":"70","line":54,"column":22,"nodeType":"63","endLine":54,"endColumn":73},{"ruleId":"66","severity":1,"message":"71","line":55,"column":22,"nodeType":"63","endLine":55,"endColumn":73},{"ruleId":"66","severity":1,"message":"72","line":57,"column":22,"nodeType":"73","endLine":57,"endColumn":23},"no-native-reassign",["74"],"no-negated-in-lhs",["75"],"no-unused-vars","'Overlay